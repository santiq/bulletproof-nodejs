{
  "name": "bulletproof-nodejs",
  "version": "1.0.0",
  "description": "Bulletproof node.js",
  "main": "src/app.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "heroku-postbuild": "npm run build",
    "serve": "node build/app.js",
    "start": "nodemon",
    "inspect": "nodemon --inspect=0.0.0.0 src/app.ts",
    "test": "jest",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "dockerize": "docker build . -t api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santiq/bulletproof-nodejs.git"
  },
  "keywords": [
    "boilerplay",
    "cron",
    "jobs",
    "js",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "author": "Santiago Quinteros",
  "license": "ISC",
  "dependencies": {
    "@useoptic/express-middleware": "^0.0.5",
    "agenda": "^4.2.1",
    "agendash": "^2.1.1",
    "argon2": "^0.28.3",
    "body-parser": "^1.19.1",
    "celebrate": "^15.0.0",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "errorhandler": "^1.5.1",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^4.0.7",
    "express": "^4.17.2",
    "express-basic-auth": "^1.2.1",
    "express-jwt": "^6.1.0",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mailgun.js": "4.1.6",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.1.7",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0",
    "tslint": "^5.20.1",
    "typescript": "^4.5.5"
  },
  "bugs": {
    "url": "https://github.com/santiq/bulletproof-nodejs/issues"
  },
  "homepage": "https://github.com/santiq/bulletproof-nodejs#readme"
